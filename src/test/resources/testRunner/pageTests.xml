                <parameter name="browser" value="chrome"/>
                <parameter name="width" value="1440"/>
                <parameter name="height" value="1864"/>
               <methods>
                   <include name="licenseShouldBeValid"/>
               </methods>
            </class>
        </classes>
    </test>
</suite>
